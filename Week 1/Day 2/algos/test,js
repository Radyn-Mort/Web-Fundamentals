var x = 5;
    
function setX(newValue) {
    x = newValue; 
}
    
console.log(x);
setX(15);
console.log(x);
// var | Val
// x   | 5,15

//consol |5,15

var x = 5;
    
function addToX(amount) {
    return x + amount;
    console.log("hello there");
}
    
console.log(x);
var result = addToX(-10);
console.log(result);
console.log(x);

//var   | Val
//x     |5
//amount|-10
//result|-5

//console|5,-5,5


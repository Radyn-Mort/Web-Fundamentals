
array=['a', 'b', 'c', 'd', 'e'];
let i=0;
let t=array.length -1;
    while( i < array.length){
        let temp = array[i];
        array[i]=array[t];
        array[t]=temp;
        // console.log(array[i])
        // console.log(temp)
        i ++;
        t --;
        
    }

console.log(array)
console.log(array[0])